    "## Build the team name to team_id map\n",
    "list_teams = ['ATL', 'BOS', 'CLE', 'NOP', 'CHI', 'DAL', 'DEN', 'GSW', 'HOU', 'LAC', 'LAL'\n",
    "        , 'MIA', 'MIL', 'MIN', 'BKN', 'NYK', 'ORL', 'IND', 'PHI', 'PHX', 'POR', 'SAC'\n",
    "        , 'SAS', 'OKC', 'TOR', 'UTA', 'MEM', 'WAS', 'DET', 'CHA']\n",
    "i = 0\n",
    "team_to_id = {}\n",
    "for t in list_teams:\n",
    "    team_to_id.update({t: nba_teams[i]['id']})\n",
    "    i += 1\n",
    "print(team_to_id)"
   ]
